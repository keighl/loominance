{
  "name": "loominance",
  "version": "1.0.0",
  "description": "Luminance and contrast functions based on WCAG 2.0",
  "main": "lib/loominance.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js",
		"lint": "./node_modules/eslint/bin/eslint.js ./src/** --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keighl/loominance.git"
  },
  "keywords": [
    "contrast",
		"color",
		"luminance"
  ],
  "author": "Kyle Truscott",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keighl/loominance/issues"
  },
  "homepage": "https://github.com/keighl/loominance",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3",
		"eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1"
  }
}
