{
    "extends": "airbnb",
    "rules": {
      "semi": 0,
      "no-tabs": 0,
      "indent": [2, "tab"],
      "react/prop-types": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-indent": [2, "tab"],
      "react/jsx-indent-props": [2, "tab"],
      "jsx-a11y/anchor-is-valid": 0,
			"no-use-before-define": 0,
			"prefer-destructuring": [
				"error",
				{
					"object": true, "array": false
				}
			]
    }
}
